    "cpx": "^1.5.0",
    "css-loader": "^3.4.2",
    "ejs-compiled-loader": "^1.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "json-loader": "^0.5.7",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cssnext": "^3.1.0",
